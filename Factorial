#include<stdio.h>
#include<stdlib.h>
// factorial(5)=5!=5*4*3*2*1=120
int factorial(int n){
    if (n==0){
        return 1;
    }
    
    if (n==1){
        return 1;
    }
    return n*factorial(n-1);
}
int main(int argc, char const*argv[])
{
    printf("%d\n", factorial(atoi(argv[1])));
    return 0;
}
